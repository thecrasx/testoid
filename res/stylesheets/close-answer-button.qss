QPushButton {
    border: none;
    background-color: transparent;
    icon: url("./assets/icons/round-close-answer-normal.png");
    icon-size: 32px;
}

QPushButton::hover {
    icon: url("./assets/icons/round-close-answer-hover.png");
}


QPushButton::pressed {
    icon: url("./assets/icons/round-close-answer-select.png");
}