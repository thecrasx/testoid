QPushButton {
	background-color: #875DA8;
	border: none;	
    border-left: 1px solid #5C3E74;
	border-bottom-right-radius: 15px;
	padding: 0px 20px 0px 20px;
    icon: url("./res/icons/tv-next-btn-normal.png");
    icon-size: 28px;
}


QPushButton::hover {
    icon: url("./res/icons/tv-next-btn-hover.png");
}


QPushButton::pressed {
    icon: url("./res/icons/tv-next-btn-select.png");
}