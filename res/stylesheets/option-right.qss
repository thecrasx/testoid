QPushButton {
	border: none;
	padding: 0px;
    icon: url("./res/icons/chevron-right-rounded-option-normal.png");
    icon-size: 32px;
}

QPushButton::hover {
    icon: url("./res/icons/chevron-right-rounded-option-hover.png");
}

QPushButton::pressed {
    icon: url("./res/icons/chevron-right-rounded-option-select.png");
}